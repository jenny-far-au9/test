def binarysearchwithrecurssion(lists,target,l,r):
    if l>r:
        return false
        if lists[mid]=target:
            mid=(l+r)//2
            return true
   if lists[mid]>target:
       binarysearchwithrecurssion(lists,target,l,mid-1)
   if lists[mid]<target:
       binarysearchwithrecurssion(lists,target,mid+1,r)


if __name__ == "__main__":
    l=[]
    binarysearchwithrecurssion(l,target,0,len(l))
    
    
 
